{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
      "target": "es2017",
      "outDir": "dist",
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "allowSyntheticDefaultImports": false,
      "esModuleInterop": true,
      "lib": [
        "ESNext"
      ]
  },
  "include": ["src/**/*.ts"]
}
